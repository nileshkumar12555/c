/******************************************************************************

write a program to find the prefect square fromm 1 to 500

*******************************************************************************/
#include <stdio.h>
#include<math.h>

int main()
{
  int i,count,x;
  float c;
  printf(" prefect square fromm 1 to 500 :");
  count=0;
  for(i=1;i<=500;i++)
  {
    c=sqrt(i);
    x=floor(c);//for rounding the value
    if(c==x)
    {
      printf("\t%d",i);
      count++;
    }
  }
  printf("\n total perfect square : =%d \n",count);
    return 0;
}

