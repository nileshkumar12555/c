/******************************************************************************

write a program to find the number in between 7 qnd 100 which is exactly divides 
by 4 and if divided by 5 and 6 remainders obtained should be 4

*******************************************************************************/
#include <stdio.h>

int main()
{
  int x;
  for(x=7;x<100;x++)
  {
    if(x%4==0 && x%5==4 && x%6==4)
    {
      printf("\n number::%d",x);
    }
  }

    return 0;
}

